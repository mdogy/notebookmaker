      "equations": [
        {
          "latex": "V_{nr} = \\frac{n!}{(n-k)!} = \\frac{365!}{(365-23)!}",
          "description": "Number of ways to choose k birthdays from n days without replacement"
        },
        {
          "latex": "V_t = n^k = 365^{23}",
          "description": "Total number of possible birthday combinations"
        },
        {
          "latex": "P(A) = \\frac{V_{nr}}{V_t} \\approx 0.492703",
          "description": "Probability of no shared birthdays"
        },
        {
          "latex": "P(B) = 1 - P(A) \\approx 1 - 0.492703 \\approx 0.507297 (50.7297\\%)",
          "description": "Probability of at least one shared birthday"
        }
      ],
